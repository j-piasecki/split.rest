    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@components/*": ["components/*"],
      "@styling/*": ["styling/*"],
      "@type/*": ["type/*"],
      "@utils/*": ["utils/*"],
      "@database/*": ["database/*"]
    }
  },
  "exclude": ["node_modules", "dist"]
}
